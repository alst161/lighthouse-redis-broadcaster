{
  "name": "alst161/lighthouse-redis-broadcaster",
  "description": "A lighthouse-php redis broadcaster to be used with laravel-echo-server.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    { 
      "name": "Konstantin Duczek",
      "email": "konz.duczek@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nuwave/lighthouse": "^4",
    "illuminate/support": "^8",
    "illuminate/console": "^8",
    "illuminate/cache": "^8",
    "illuminate/redis": "^8",
    "illuminate/broadcasting": "^8",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "laravel/framework": "^8",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "alst161\\LighthouseRedisBroadcaster\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
